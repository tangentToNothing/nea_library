<p id="notice"><%= notice %></p>

<h1>Listing Targetgroups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @targetgroups.each do |targetgroup| %>
      <tr>
        <td><%= targetgroup.name %></td>
        <td><%= link_to 'Show', targetgroup %></td>
        <td><%= link_to 'Edit', edit_targetgroup_path(targetgroup) %></td>
        <td><%= link_to 'Destroy', targetgroup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Targetgroup', new_targetgroup_path %>
